# Environment Variables Template
# Copy this file to .env.local for development or .env.production for production
# Replace the placeholder values with your actual configuration

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Feature Flags
NEXT_PUBLIC_ENABLE_DEVTOOLS=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# Kakao OAuth Configuration
# Get these values from https://developers.kakao.com/
NEXT_PUBLIC_KAKAO_CLIENT_ID=your_kakao_app_key_here
NEXT_PUBLIC_KAKAO_REDIRECT_URI=http://localhost:3000/auth/kakao/callback

# Environment
NODE_ENV=development